<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"


    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/tishabak"

    >


    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="172dp"
        android:layout_height="100dp"
        android:layout_marginLeft="100dp"

        android:background="@drawable/single"
        android:onClick="welcombtsingleplayer"
        app:srcCompat="@drawable/trp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="172dp"
        android:layout_height="100dp"
        android:layout_marginLeft="100dp"

        android:background="@drawable/towplayers"
        android:onClick="welcombttowplayer"
        app:srcCompat="@drawable/trp" />


    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="172dp"
        android:layout_height="100dp"
        android:layout_marginLeft="100dp"

        android:background="@drawable/howtoplay"
        android:onClick="welcomehowtoplay"
        app:srcCompat="@drawable/trp" />


    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="172dp"
        android:layout_height="100dp"
        android:layout_marginLeft="100dp"

        android:background="@drawable/contact"
        android:onClick="welcomecontact"
        app:srcCompat="@drawable/trp" />


    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:checked="true"
        android:duplicateParentState="false"
        android:text="Sound"
        android:visibility="visible" />

    <RelativeLayout
        android:id="@+id/rlawil"
        android:layout_width="match_parent"
        android:layout_height="50dp"

        android:layout_marginTop="1dp">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yt"
        android:layout_width="match_parent"
        android:layout_height="40dp">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/yy"
        android:layout_marginTop="40dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </RelativeLayout>


</LinearLayout>
